        <item>-%s</item>      <!-- format_folder_name -->
        <item>--%s</item>     <!-- format_folder_note_date -->
        <item>--%s</item>     <!-- format_note_date -->
        <item>--%s</item>     <!-- format_note_content -->
    </string-array>

    <string-array name="contact_entries">
        <item>Name</item>
